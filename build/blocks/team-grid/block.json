    "iconSize": {
      "type": "number",
      "default": 18
    },
    "iconsSpacing": {
      "type": "string",
      "default": "5"
    },
    "iconsPanelBg": {
      "type": "string",
      "default": "#d4d5ff"
    },
    "iconColor": {
      "type": "string",
      "default": "#4f4f7c"
